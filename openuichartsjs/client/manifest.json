{
  "_version": "1.3.0",
  "sap.app": {
    "_version": "1.3.0",
    "id": "openui5_chartjs",
    "type": "application",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "dataSources": {
      "chartcoll_ds": {
        "uri": "model/dashboard.json",
        "type": "JSON"
      }
    }

  },
  "sap.ui": {
    "_version": "1.3.0",
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    },
    "supportedThemes": [
      "sap_bluecrystal"
    ]
  },
  "sap.ui5": {
    "_version": "1.2.0",
    "rootView": "openui5_chartjs.view.app",

    "config": {
      "chartjsModel": "/model/data_chartjs.json",
      "c3jsModel": "/model/data_c3js.json"
    },

    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "openui5_chartjs.i18n.i18n"
        }
      },
      "dbtiles_model": {
        "type": "sap.ui.model.json.JSONModel",
        "dataSource": "chartcoll_ds"
      }


    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "openui5_chartjs.view",
        "controlId": "rootControl",
        "controlAggregation": "pages",
        "transition": "slide"
      },
      "routes": [{
          "pattern": "",
          "name": "cdb",
          "target": "cdb"

        },
        {
            "pattern": "linechart",
            "name": "linechart",
            "target": "linechart"

          }


      ],
      "targets": {
        "cdb": {
          "viewName": "cdb",
          "viewLevel": 1
        },
        "linechart": {
          "viewName": "linechart",
          "viewLevel": 1
        }


      },
      "resources": {
        "css": [{
          "uri": "css/c3.min.css"
        }, {
          "uri": "css/style.css"
        }],
        "js": [{
          "uri": "/control/Chart.min.js"
        }, {
          "uri": "/control/d3.min.js"
        }, {
          "uri": "/control/c3.min.js"
        }]
      }



    }
  }
}
