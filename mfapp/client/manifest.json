{
	"_version": "1.3.0",
	"sap.app": 
	{
		"_version": "1.3.0",
		"id": "simple_hello",
		"type": "application",
		"applicationVersion": 
		{
			"version": "1.0.0"
		},
		"dataSources":{
			"navbar_ds": {
				"uri": "models/navbar.json",
				"type": "JSON"
			},
			"dbrdtiles_ds": {
				"uri": "models/dboardtiles.json",
				"type": "JSON"
			},
			"mfinvmodel_ds": {
				"uri": "models/mfinvmodel.json",
				"type": "JSON"
			}

		}
	},
	"sap.ui": 
	{
		"_version": "1.3.0",
		"technology": "UI5",
		"deviceTypes": 
		{
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": 
		[
			"sap_bluecrystal"
		]
	},
	"sap.ui5": 
	{
		"_version": "1.2.0",
		"rootView": "simple_hello.view.app",
		
		"models":
		{
			"i18n":
			{
				"type":"sap.ui.model.resource.ResourceModel",
				"settings":
				{
					"bundleName":"simple_hello.i18n.i18n"
				}
			},
			"navbar_model":{
				"type":"sap.ui.model.json.JSONModel",
				"dataSource":"navbar_ds"
			},
			"dbtiles_model":{
				"type":"sap.ui.model.json.JSONModel",
				"dataSource":"dbrdtiles_ds"
			},
			"mfinv_model":{
				"type":"sap.ui.model.json.JSONModel",
				"dataSource":"mfinvmodel_ds"
			},
			"usrlgn_model":{
				"type":"sap.ui.model.json.JSONModel"
			},
			"scheme_model":{
				"type":"sap.ui.model.json.JSONModel"
			},
			"amc_model":{
				"type":"sap.ui.model.json.JSONModel"
			},
			"post_scheme_model":{
				"type":"sap.ui.model.json.JSONModel"
			},
			"post_amc_model":{
				"type":"sap.ui.model.json.JSONModel"
			},
			"post_nav_model":{
				"type":"sap.ui.model.json.JSONModel"
			},
			"vis_model":{
				"type":"sap.ui.model.json.JSONModel"
			},
			"schdet_model":{
				"type":"sap.ui.model.json.JSONModel"
			},
			"selSchModel":{
				"type":"sap.ui.model.json.JSONModel"
			}
		},
		"routing":
		{
			"config":
			{
				"routerClass": "sap.m.routing.Router",
	            "viewType": "XML",
	            "viewPath": "simple_hello.view",
	            "controlId": "rootControl",
	            "controlAggregation": "pages",
	            "transition": "slide"
	         },
	         "routes":[{
	        	 "pattern": "",
	             "name": "login",
	             "target": "login"

	         },
	         
	         {
	        	 "pattern": "dashboard",
	             "name": "dashboard",
	             "target": "dashboard"

	         },
	         
	         {
	        	 "pattern": "chatenmf",
	             "name": "chatenmf",
	             "target": "chatenmf"

	         },
	         
	         {
	        	 "pattern": "admin",
	             "name": "admin",
	             "target": "admin"

	         },
	         
	         {
	        	 "pattern": "register",
	             "name": "register",
	             "target": "register"

	         },
	         {
	        	 "pattern": "getschemes",
	             "name": "getschemes",
	             "target": "getschemes"

	         },
	         {
	        	 "pattern": "getamcs",
	             "name": "getamcs",
	             "target": "getamcs"

	         },
	         {
	        	 "pattern": "addschemes",
	             "name": "addschemes",
	             "target": "addschemes"

	         },
	         {
	        	 "pattern": "addamcs",
	             "name": "addamcs",
	             "target": "addamcs"

	         },
	         {
	        	 "pattern": "addnav",
	             "name": "addnav",
	             "target": "addnav"

	         },
	         {
	        	 "pattern": "mfinvlog",
	             "name": "mfinvlog",
	             "target": "mfinvlog"

	         },
	         {
	        	 "pattern": "schdet/{scode}",
				 "name": "schdet",
				 "target": "schdetails"
	         }],
	         "targets": {
	             "login": {
	                "viewName": "login",
	                "viewLevel" : 1
	             },
	             "dashboard": 
	             {
		                "viewName": "dashboard",
		                "viewLevel" : 1
		         },
		         "chatenmf": 
	             {
		                "viewName": "chatenmfdb",
		                "viewLevel" : 1
		         },
		         "admin": 
	             {
		                "viewName": "admin",
		                "viewLevel" : 1
		         },
		         "register": 
	             {
		                "viewName": "register",
		                "viewLevel" : 1
		         },
		         "getschemes": 
	             {
		                "viewName": "getschemes",
		                "viewLevel" : 1
		         },
		         "getamcs": 
	             {
		                "viewName": "getamcs",
		                "viewLevel" : 1
		         },
		         "addschemes": 
	             {
		                "viewName": "addschemes",
		                "viewLevel" : 1
		         },
		         "addamcs": 
	             {
		                "viewName": "addamcs",
		                "viewLevel" : 1
		         },
		         "addnav": 
	             {
		                "viewName": "addnav",
		                "viewLevel" : 1
		         },
		         "schdetails": 
	             {
		                "viewName": "schdetails",
		                "viewLevel" : 1
		         },
		         "mfinvlog": 
	             {
		                "viewName": "mfinvlog",
		                "viewLevel" : 1
		         }
		         
	          }


			
		}
	}
}