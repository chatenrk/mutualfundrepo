{
	"_version": "1.3.0",
	"sap.app": 
	{
		"_version": "1.3.0",
		"id": "simple_hello",
		"type": "application",
		"applicationVersion": 
		{
			"version": "1.0.0"
		},
		"dataSources":{
			"navbar_ds": {
				"uri": "models/navbar.json",
				"type": "JSON"
			},
			"dbrdtiles_ds": {
				"uri": "models/dboardtiles.json",
				"type": "JSON"
			}

		}
	},
	"sap.ui": 
	{
		"_version": "1.3.0",
		"technology": "UI5",
		"deviceTypes": 
		{
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": 
		[
			"sap_bluecrystal"
		]
	},
	"sap.ui5": 
	{
		"_version": "1.2.0",
		"rootView": "simple_hello.view.app",
		
		"models":
		{
			"i18n":
			{
				"type":"sap.ui.model.resource.ResourceModel",
				"settings":
				{
					"bundleName":"simple_hello.i18n.i18n"
				}
			},
			"navbar_model":{
				"type":"sap.ui.model.json.JSONModel",
				"dataSource":"navbar_ds"
			},
			"dbtiles_model":{
				"type":"sap.ui.model.json.JSONModel",
				"dataSource":"dbrdtiles_ds"
			},
			"usrlgn_model":{
				"type":"sap.ui.model.json.JSONModel"
			}
		},
		"routing":
		{
			"config":
			{
				"routerClass": "sap.m.routing.Router",
	            "viewType": "XML",
	            "viewPath": "simple_hello.view",
	            "controlId": "rootControl",
	            "controlAggregation": "pages",
	            "transition": "slide"
	         },
	         "routes":[{
	        	 "pattern": "",
	             "name": "login",
	             "target": "login"

	         },
	         {
	        	 "pattern": "dashboard",
	             "name": "dashboard",
	             "target": "dashboard"

	         },
	         {
	        	 "pattern": "register",
	             "name": "register",
	             "target": "register"

	         }],
	         "targets": {
	             "login": {
	                "viewName": "login",
	                "viewLevel" : 1
	             },
	             "dashboard": 
	             {
		                "viewName": "dashboard",
		                "viewLevel" : 1
		         },
		         "register": 
	             {
		                "viewName": "register",
		                "viewLevel" : 1
		         }
	          }


			
		}
	}
}