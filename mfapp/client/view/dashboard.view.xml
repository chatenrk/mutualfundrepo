<View
	xmlns="sap.m"
	xmlns:layout="sap.ui.layout"
	controllerName="simple_hello.Controller.dashboard">
	
		<TileContainer id="container" tileDelete="handleTileDelete"
            tiles="{dbtiles_model>/TileCollection}">
 
            <StandardTile icon="{dbtiles_model>icon}" type="{dbtiles_model>type}" number="{dbtiles_model>number}"
                numberUnit="{dbtiles_model>numberUnit}" title="{dbtiles_model>title}" info="{dbtiles_model>info}" infoState="{dbtiles_model>infoState}"
                press="onTilePressed" />
        </TileContainer>
	
</View>