<View
	xmlns="sap.m"
	xmlns:layout="sap.ui.layout"
	controllerName="simple_hello.Controller.dashboard">
	<ObjectHeader
		title="Scheme related" />
	<HeaderContainer orientation="Horizontal">
	
	
	
	<content>
	<FlexBox items = "{dbtiles_model>/SchemeCollection}">
	<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" 
				 header="{dbtiles_model>title}"
				 press="onTilePress">
		<TileContent>
			<ImageContent src="{dbtiles_model>icon}" />
		</TileContent>
	</GenericTile>
	</FlexBox>	
	</content>
	
	</HeaderContainer>
	
	<ObjectHeader
		title="AMC related" />
	<HeaderContainer orientation="Horizontal">
	<content>
	<FlexBox items = "{dbtiles_model>/AMCCollection}">
	<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" 
				 header="{dbtiles_model>title}"
				 press="onTilePress">
		<TileContent>
			<ImageContent src="{dbtiles_model>icon}" />
		</TileContent>
	</GenericTile>
	</FlexBox>	
	</content>
	
	</HeaderContainer>
	
</View>