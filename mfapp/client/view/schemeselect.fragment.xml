<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		noDataText="No Schemes Found"
		title="Select Scheme"
		liveChange="handleSearch"
		confirm="handleClose"
		cancel="handleClose"
		items="{selschModel>/}" >
		<StandardListItem
			title="{selschModel>sname}"
			type="Active" />
	</SelectDialog>
</core:FragmentDefinition>