<mvc:View controllerName="trello_sample.controller.dashboard" xmlns:l="sap.ui.layout" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m">
  <App id="app">
    <HeaderContainer orientation="Horizontal">
      <content>
        <FlexBox items="{boards_model>/}">
          <GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="{boards_model>name}" press="onTilePress" frameType="TwoThirds">
            <tileContent>
              <TileContent>
                <content>
                  <Image src = "https://trello-backgrounds.s3.amazonaws.com/SharedBackground/140x100/360ee286473b45416f0164202c82ae57/photo-1520716448897-f1cc6b0241ce.jpg"/>
                </content>
              </TileContent>
            </tileContent>
          </GenericTile>
        </FlexBox>
      </content>
    </HeaderContainer>
  </App>
</mvc:View>
