<mvc:View controllerName="trello_sample.controller.dashboard" xmlns:l="sap.ui.layout" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m">
  <App id="app">
    <HeaderContainer orientation="Horizontal">
      <content>
        <FlexBox items="{boards_model>/}">
          <GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="{boards_model>name}" press="onTilePress">
          </GenericTile>
        </FlexBox>
      </content>
    </HeaderContainer>
  </App>
</mvc:View>
