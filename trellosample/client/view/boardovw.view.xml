<mvc:View controllerName="trello_sample.controller.boardovw" xmlns:l="sap.ui.layout" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m">
  <ObjectPageLayout id="ObjectPageLayout" enableLazyLoading="false" showTitleInHeaderContent="true"></ObjectPageLayout>
  <headerTitle>
    <ObjectPageHeader
      id="objpghdr"
      objectImageURI="{schdet_model>/imgpath}"
      objectTitle="{schdet_model>/sname}"
      objectImageShape="Square"
      objectImageAlt="{schdet_model>/fhouse}"
      isObjectIconAlwaysVisible="false"
      isObjectTitleAlwaysVisible="false"
      isObjectSubtitleAlwaysVisible="false"></ObjectPageHeader>
  </mvc:View>
