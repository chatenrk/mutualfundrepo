{
  "_version": "1.3.0",
  "sap.app": {
    "_version": "1.3.0",
    "id": "trello_sample",
    "type": "application",
    "applicationVersion": {
      "version": "1.0.0"
    }
  },
  "sap.ui": {
    "_version": "1.3.0",
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    },
    "supportedThemes": [
      "sap_bluecrystal"
    ]
  },
  "sap.ui5": {
    "_version": "1.2.0",
    "rootView": "trello_sample.view.dashboard",
    "resources": {
      "css": [{
        "uri": "css/style.css"
      }]
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "trello_sample.i18n.i18n"
        }
      },
      "boards_model": {
        "type": "sap.ui.model.json.JSONModel"
      }

    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "trello_sample.view",
        "controlId": "app",
        "controlAggregation": "pages",
        "transition": "slide"
      },
      "routes": [{
        "pattern": "",
        "name": "TrelloDashbaord",
        "target": "Dashboard"
      }],
      "targets": {
        "Dashboard": {
          "viewName": "dashboard",
          "viewLevel": 1
        }

      }

    }
  }
}
