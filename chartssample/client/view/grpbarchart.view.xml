<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout" xmlns="sap.m" controllerName="charts_sample.controller.grpbarchart" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Group Bar Chart" class="sapUiFioriObjectPage" showNavButton="true" navButtonPress="onNavBack">
		<l:VerticalLayout class="panelBoxContainer sapUiFioriObjectPage" width="100%">
			<l:content>
				<Panel>
					<headerToolbar>
						<Toolbar>
							<Label text="{Group Bar Chart}"/>
						</Toolbar>
					</headerToolbar>
					<FlexBox id="ChartHolder" alignItems="Start" justifyContent="Center"></FlexBox>
				</Panel>

			</l:content>
		</l:VerticalLayout>
	</Page>
</core:View>
