<!-- This shows a multiline chart where both the line data is defined in the same Data JSON. Tough to scale for multiple
entries and needs to be done better -->

<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout" xmlns="sap.m" controllerName="charts_sample.controller.mullinechtgwt" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Cost Versus Value Line Chart" class="sapUiFioriObjectPage" showNavButton="true" navButtonPress="onNavBack">
		<l:VerticalLayout class="panelBoxContainer sapUiFioriObjectPage" width="100%">
			<l:content>
				<Panel>
					<headerToolbar>
						<Toolbar>
							<Label text="Cost Versus Value Line Chart"/>
						</Toolbar>
					</headerToolbar>
					<FlexBox id="ChartHolder" alignItems="Start" justifyContent="Center"></FlexBox>
				</Panel>
			</l:content>
		</l:VerticalLayout>
	</Page>
</core:View>
