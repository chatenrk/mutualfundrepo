<mvc:View xmlns="sap.m" xmlns:layout="sap.ui.layout" xmlns:mvc="sap.ui.core.mvc" controllerName="charts_sample.controller.chtdashboard">
<Page title="Charts Dashboard">
	<ObjectHeader title="Charts Dashboard"/>
	<HeaderContainer orientation="Horizontal">
		<content>
			<FlexBox items="{dbtiles_model>/ChartsCollection}">
				<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="{dbtiles_model>title}" press="onTilePress">
					<TileContent>
						<ImageContent src="{dbtiles_model>icon}"/>
					</TileContent>
				</GenericTile>
			</FlexBox>
		</content>
	</HeaderContainer>
</Page>
</mvc:View>
