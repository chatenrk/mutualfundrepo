{
  "_version": "1.3.0",
  "sap.app": {
    "_version": "1.3.0",
    "id": "charts_sample",
    "type": "application",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "dataSources": {
      "chartcoll_ds": {
        "uri": "models/dboardtiles.json",
        "type": "JSON"
      }
  }
},
  "sap.ui": {
    "_version": "1.3.0",
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    },
    "supportedThemes": [
      "sap_bluecrystal"
    ]
  },
  "sap.ui5": {
    "_version": "1.2.0",
    "rootView": "charts_sample.view.app",
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "charts_sample.i18n.i18n"
        }
      },
      "dbtiles_model": {
        "type": "sap.ui.model.json.JSONModel",
        "dataSource": "chartcoll_ds"
      },
      "simlinecht_model": {
        "type": "sap.ui.model.json.JSONModel"
      }
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "charts_sample.view",
        "controlId": "rootControl",
        "controlAggregation": "pages",
        "transition": "slide"
      },
      "routes": [{
        "pattern": "",
        "name": "Chart Tiles",
        "target": "Tiles"

      },
      {
        "pattern": "Line Chart",
        "name": "simplelinechart",
        "target": "simlinecht"

      }
    ],
      "targets": {
        "Tiles": {
          "viewName": "chtdashboard",
          "viewLevel": 1
        },
        "simlinecht": {
          "viewName": "simlinecht",
          "viewLevel": 1
        }

      }

    }
  }
}
